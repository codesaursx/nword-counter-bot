// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CountTracker {
  userId  String @unique
  guildId String @unique
  count   Int    @default(0)

  @@id([userId, guildId])
  @@map("count_tracker")
}
